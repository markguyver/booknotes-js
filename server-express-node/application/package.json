{
  "name": "booknotes-js-server",
  "version": "0.2.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "compile": "rm -rf dist && tsc",
    "db:migrate": "cd database && sequelize db:migrate",
    "db:rollback": "cd database && sequelize db:migrate:undo",
    "develop": "npm run compile && nodemon dist",
    "documentation": "redoc-cli bundle redoc.json",
    "lint": "npm run compile && eslint dist",
    "minimize": "uglifyjs -c -m -o dist/index.min.js -- $(find /var/node/dist -type f -iname *.js)",
    "start": "node dist/index.js"
  },
  "author": "Mark Chavez <mark.chavez@startmail.com>",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-pino-logger": "^5.0.0",
    "immutable": "^4.0.0-rc.14",
    "mysql2": "^2.1.0",
    "pino": "^6.11.3",
    "ramda": "^0.27.1",
    "sequelize": "^5.22.3"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.36",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-pino-logger": "^4.0.2",
    "@types/node": "^13.13.52",
    "@types/pino": "^6.3.9",
    "@types/ramda": "^0.26.44",
    "@types/sequelize": "^4.28.10",
    "@types/validator": "^12.0.1",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "eslint": "^6.8.0",
    "nodemon": "^2.0.11",
    "redoc": "^2.0.0-rc.54",
    "sequelize-cli": "^5.5.1",
    "typescript": "^3.9.10",
    "uglify-js": "^3.13.10"
  }
}
